{
    "name": "clearcodehq/command-bus-launcher-bundle",
    "type": "library",
    "description": "Automaticly launch CommandBus commands",
    "license": "MIT",
    "authors": [
        {
            "name": "ClearcodeHQ",
            "homepage": "http://clearcode.cc/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "simple-bus/symfony-bridge": "^4.1",
        "clearcodehq/command-bus-launcher": "dev-master",
        "symfony/framework-bundle": "~2.3",
        "symfony/console": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/config": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "^0.6.1"
    },
    "autoload": {
        "psr-4" : { "ClearcodeHQ\\CommandBusLauncherBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4" : { "tests\\ClearcodeHQ\\CommandBusLauncherBundle\\" : "tests" }
    }
}
